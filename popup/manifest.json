{
  "manifest_version": 3,
  "name": "TabTrackr",
  "version": "1.0",
  "description": "Group, save, and track your Chrome tabs with AI.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "../background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "TabTracker"
  }
} 